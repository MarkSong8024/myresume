#pageList .homePage {
    background-color: rgb(255, 255, 255);
    margin: 15px;
    box-shadow: 2px 2px 0 #f2f2f2;
    border: 1px solid rgb(193, 193, 193);
}

#pageList .homePage .infoTitle {
    margin: 16px;
    /*font-family: STHeitiSC-Medium;*/
    font-size: 22px;
    color: rgb(0 0 0);
    margin-bottom: 27px;
}

#pageList .homePage .infoContext {
    /*width: 20em;*/
    margin: 16px;
    font-size: 14px;
    color: rgb(128 128 128);
    overflow: hidden;
    margin-right: 44px;
    text-overflow: ellipsis;
    /*white-space:nowrap;*/

}

#pageList .homePage .homeDir {
    position: relative;
    top: -30px;
    width: 15px;
    background: url("images/icon_right.svg") no-repeat;
