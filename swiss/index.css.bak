* {
    margin: 0;
    padding: 0;
    -webkit-user-select: none;
    user-select: none;
}

html, body {
    /*width: 100%;*/
    /*height: 100%;*/
    /*font-size: 14px;*/

}

.context {
    display: none;
    overflow: hidden;

}

/*��һҳ*/
#pageHome {
    display: block;
    background: url("images/Background.jpg") right center no-repeat;
    background-size: cover;
    padding-bottom: 200px;
    overflow: hidden;
}

.first {
    border-top: 35px solid #fff;
    border-left: 370px solid transparent;
}

.logo {
    height: 82px;
}

.logo img {
    position: relative;
    width: 82px;
    height: 82px;
    float: right;
    margin-top: -35px;
}

.title {
    /*width: 100%;*/
    margin-left: 30px;
}

.title .main {
    box-shadow: rgba(0 0 0 0.7);
    color: rgb(255, 255, 255);
}

.title .main .p1 {
    /*letter-spacing: 2px;*/
    font-size: 24px;
    height: 32px;
    line-height: 32px;
}

.title .main .p2 {
    margin-top: 10px;
    font-size: 14px;
}

.title .main1 .ul1 li a {
    text-decoration: none;
    font-size: 14px;
    color: rgb(255, 255, 255);
}

.title .main1 {
    margin-top: 30px;
    /*margin-right: 210px;*/
}

.title .main1 ul li {
    width: 50%;
    list-style: none;
    border: 1px solid rgb(243, 243, 243);
    color: rgb(255, 255, 255);
    background-color: rgba(200, 196, 196, 0.3);
    margin-top: 15px;

}

.title .main1 ul li a {
    padding: 18px;
    position: relative;
    display: block;
}

.title .main1 ul li a img {
    position: absolute;
    right: 15px;
}

.div2 {
    width: 70%;
    height: 25%;
    background-color: rgb(255, 255, 255);
    /*position: fixed;*/
    position: absolute;
    top: 50%;
    left: 50%;
    margin-top: -22%;
    margin-left: -34%;
    display: none;
    text-align: center;
    z-index: 3;
    overflow: hidden;
}

.div2 input {
    border: 1px solid #2f2f2f;
    background-color: rgb(255, 255, 255);
    margin-left: 25px;
    padding: 8px 15px;
    outline: none;
}

.div2 p {
    color: #2f2f2f;
    margin: 8px auto;
    padding: 5px;
    margin-bottom: 20px;
    margin-left: 5px;
}

.keepOut {
    height: 100%;
    width: 100%;
    background-color: rgba(200, 196, 196, 0.8);
    display: none;
    position: absolute;
    top: 0;
    z-index: 2;
}

/*�ڶ�ҳ*/
#pageInfo {
    /*display: block;*/
    background-image: url("images/Background_blur.jpg");
    background-size: cover;
    padding-bottom: 200px;
}

.total {
    height: 68px;
    background-color: rgb(255, 255, 255);
}

.total .headS {
    width: 52px;
    height: 52px;
    margin: 8px 15px 8px;
    background-color: transparent;
    border-radius: 50%;
    border: 1px solid red;
    float: left;
    box-sizing: border-box;
}

.total .headC {
    margin-left: 82px;
    padding-top: 13px;
}

.context .total .p1 {
    display: inline-block;
    margin: 13px 0 0 82px;
    font-size: 22px;
}

.context .total .p2 {
    display: inline-block;
    width: 28px;
    height: 14px;
    margin: 41px 264px 13px 82px;
    font-size: 14px;
}

.head-city {
    font-size: 14px;
    margin-top: 5px;
}

.context .pres {
    margin-top: 30px;
}

.context .pres ul li {
    margin: 20px 30px 0;
    list-style: none;
    border: 1px solid rgb(243, 243, 243);
    color: rgb(255, 255, 255);
    font-size: 14px;
    padding: 16px;
}

.pres ul li input[type="text"] {
    border: none;
    width: 100%;
    outline: none;
    background-color: transparent;
    font-size: 14px;
    color: #FFF;
}

.pres ul li input[type="text"]::input-placeholder {
    color: #FFF !important;
}

.pres ul li input[type="text"]::-webkit-input-placeholder {
    color: #FFF !important;
}

.pres ul li input[type="button"] {
    border: none;
    outline: none;
    background-color: transparent;
    color: rgb(255, 255, 255);

    font-size: 14px;
}

.pres ul .submit {

    text-align: center;

}

